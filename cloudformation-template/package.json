{
  "name": "desole",
  "private": true,
  "version": "1.0.0",
  "description": "Shell package for cloudformation deployment, not intended to be used directly",
  "main": "lambda.js",
  "scripts": {
    "prepackage": "find ./node_modules/ -exec touch '{}' \\;",
    "package": "aws cloudformation package --template-file template.yaml --output-template-file output.yaml --s3-bucket $npm_package_config_bucket_name",
    "deploy": "aws cloudformation deploy --template-file output.yaml --stack-name $npm_package_config_cloudformation_stack --capabilities CAPABILITY_IAM --parameter-overrides PinpointApplicationId=$npm_package_config_pinpoint_id"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@desole/s3-publisher": "file:../s3-publisher",
    "@desole/pinpoint-publisher": "file:../pinpoint-publisher",
    "@desole/cloudwatch-publisher": "file:../cloudwatch-publisher",
    "@desole/collector-api": "file:../collector-api"
  },
  "config": {
    "bucket_name": "desole-packaging",
    "cloudformation_stack": "desoleStack",
    "pinpoint_id": ""
  }
}
